{
    "name": "smkn1purwosari-web",
    "version": "1.0.0",
    "main": "index.js",
    "devDependencies": {
      "autoprefixer": "^10.4.2",
      "concat": "^1.0.3",
      "live-server": "^1.2.1",
      "node-sass": "^7.0.1",
      "npm-run-all": "^4.1.5",
      "postcss-cli": "^9.1.0"
    },
    "scripts": {
      "start": "node-sass -w  sass/main.scss  css/style.css",
      "devserver": "live-server",
      "start:serve": "npm-run-all --parallel devserver watch:sass",
      "compile:sass": "node-sass  sass/main.scss  css/style.comp.css",
      "prefix:sass": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
      "compress:sass": "node-sass css/style.prefix.css css/style.css --output-style compressed",
      "build:sass": "npm-run-all compile:sass prefix:sass compress:sass "
    },
    "author": "rifaldi",
    "license": "ISC",
    "description": ""
  }
  